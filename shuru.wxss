@import "../../common/common.wxss"

.constellation{
  height: 1200rpx;
}
.constellation .text{
  width: 100%;
  color: #FF0000;
  font-size: 30px;
  text-align: center;
  padding-top: 60rpx;
}
.picker{
  width: 400rpx;
  height: 100rpx;
  border: 1px solid #eee;
  border-radius: 5px;
  text-align: center;
  line-height: 100rpx;
  margin: 100rpx 0 0 20%;
  color: white;
  font-size: 24px;
}
.form{
  color:azure;
  font-size: 12px;
  text-align: center;
}

.form input{
  border: 1px solid #eee;
  border-radius: 5px;
  width: 400rpx;
}
.constellation .myCon{
  margin: 100rpx 0 0 20%;
}

.constellation .startTest{
  margin: 60rpx 0 0 35%;
}
.warn{
  font-size: 14px;
  color: red;
  width: 100%;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
}